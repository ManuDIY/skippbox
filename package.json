{
  "name": "skippbox",
  "version": "0.0.1",
  "description": "A NWjs based k8s UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/skippbox/skippbox.git"
  },
  "main": "index.html",
  "scripts": {
    "start": "nw"
  },
  "keywords": [
    "k8s",
    "desktop",
    "nw.js"
